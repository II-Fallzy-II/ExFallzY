Section:NewToggle("Auto Drink+", "thingy", function(state)
    if state then

_G.yh = true

while _G.yh do
wait()
local Players = game:GetService("Players")
local Lp = Players.LocalPlayer
local Character = Lp.Character or Lp.CharacterAdded:Wait()
local Humanoid = Character.Humanoid
local Backpack = Lp.Backpack
for i,v in pairs(Backpack:GetChildren()) do 
  if v:IsA("Tool") and string.find(v.Name, "Juice+") or string.find(v.Name, "Cider+") or string.find(v.Name, "Smoothie+") or string.find(v.Name, "Lemonade+")  then 
      Humanoid:EquipTool(v)
      v:Activate()
      wait(0.1)
    end 
end
end    
    
    else
     _G.yh = false
    end
end)

