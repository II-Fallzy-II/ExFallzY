    if state then
        _G.pro = true
        while _G.pro do
            wait(1)
            for i,v in pairs(game:GetService("Workspace").Trees:GetDescendants()) do
if v:IsA("ClickDetector") then
fireclickdetector(v)
end
end
end
    else
   _G.pro = false
    end
end)

