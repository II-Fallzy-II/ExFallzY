local Tab = Window:NewTab("Beri Farms")
local Section = Tab:NewSection("Beri Farm (Check everything in level farm)")
Section:NewToggle("Beri Farm [Works in VIP servers]", "yeah", function(state)
    if state then
    local Lp = tostring(game:GetService("Players").LocalPlayer.UserId)
_G.beri = true
while _G.beri do
    wait(0.5)
local args = {[1] = "Claim", [2] = "Challenge13"}
workspace.UserData["User_"..Lp].ChallengesRemote:FireServer(unpack(args))
end
    else

    _G.beri = false

    end
end)

