local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/II-Fallzy-II/ExFallzY/main/BingBao"))()
local Window = Library.CreateLib("Senpaii Hub || Beta v2.1 -- NOT FINISHED", "DarkTheme")
