local Tab = Window:NewTab("Levels")
local Section = Tab:NewSection("Level Farm (Check all below) [Works in VIPS]")

Section:NewToggle("Get Drinks", "Barrels", function(state)
    if state then
       _G.ong = true
       
       while _G.ong do
       for i,v in pairs(game.Workspace.Barrels:GetDescendants()) do
if v:IsA("ClickDetector") then
fireclickdetector(v)
end
end
wait(0)
for i,v in pairs(game.Workspace.Island8.Kitchen:GetDescendants()) do
if v:IsA("ClickDetector") then
fireclickdetector(v)
end
end
      end 
    else
    _G.ong = false
    end
end)


Section:NewToggle("Auto Equip & Fire", "thingy", function(state)
    if state then

_G.drinks = true
while _G.drinks do
wait(5)
spawn(function()
local Players = game:GetService("Players")
local Lp = Players.LocalPlayer
local Character = Lp.Character or Lp.CharacterAdded:Wait()
local Humanoid = Character.Humanoid


for i,v in pairs(Lp.Backpack:GetChildren()) do 
if v.Name == "Crab Toy" then
v:Destroy()
end
    if v:IsA("Tool") and string.find(v.Name, "Juice") or string.find(v.Name, "Milk") or string.find(v.Name, "Cider") or string.find(v.Name, "Lemonade") or string.find(v.Name, "Smoothie") or string.find(v.Name, "Golden") then 
        Humanoid:EquipTool(v)
        wait(0.1)
        v:Activate()
        --wait()
    end 
end 
end)
end


    else
        _G.drinks = false
    end
end)



